<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mycompany.nncloudrestservice.model">
  <class name="Layer" table="layers">
      <property name="relative_number" column="relative_number"/>
      <id name="id" column="id_user" type="int">
          <generator class="native"/>
      </id>
      <set name="neurons" cascade="all">
          <key column="id_layer"/>
          <one-to-many class="Neuron"/>
      </set>
  </class>
</hibernate-mapping>
